<package>
  <description brief="sr_teleop">

    Package for teleoperating the Shadow hand/arm with a WinTracker and a Cyberglove. Containins the launch files for the teleoperation plus some modifications in the robot model.

  </description>
  <author>Robert Krug</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/teleop_launch</url>
  <depend package="wintracker"/>
  <depend package="aass_cyberglove"/>
  <depend package="cyberglove_remapper"/>
  <depend package="sr_hand"/>
  <depend package="eigen"/>
  <depend package="pr2_controller_manager"/>
  <depend package="pr2_mechanism_msgs"/>
  <depend package="tf"/>
  <depend package="tf_conversions"/>
  <depend package="sensor_msgs"/>
  <depend package="sr_robot_msgs"/>
  <rosdep name="boost"/>
</package>


